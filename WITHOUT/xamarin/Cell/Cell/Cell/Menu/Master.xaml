<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cell.Menu.Master"
                  xmlns:page="clr-namespace:Cell.Pagina">
    <MasterDetailPage.Master>
        <ContentPage Title="Menu">
            <StackLayout>
                <Button Text="TextCell" Clicked="GoPagina1"/>
                <Button Text="ImageCell" Clicked="GoPagina2"/>
                <Button Text="EntryView" Clicked="GoPagina3"/>
                <Button Text="SwitchPage" Clicked="GoPagina4"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <page:TextCellPage/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
    
</MasterDetailPage>