<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cell.Pagina.SwitchCellPage">
    <ContentPage.Content>
        <TableView>
            <TableRoot>
                <TableSection Title="Configuração">
                    <SwitchCell On="False" Text="Backup Automatico"/>
                    <SwitchCell On="True" Text="Salva Senhas"/>
                </TableSection>
            </TableRoot>
        </TableView>
    </ContentPage.Content>
</ContentPage>
    
    
    