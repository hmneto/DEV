

$('.btn-click').click(function (event) {
              ResultRoteiroSuccess('ok')
event.stopPropagation();
event.stopImmediatePropagation();
$.get('/Mensagem/PartialViewMensagem/?id=' + $(this).val() + '&tela=1', function (data) {
                                            $("#myModal").html(data);
                                            $("#myModal").modal('show');
                                        });
                                    });
