<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TipoPaginaXamarin.TipoPagina.Carousel.Page3"
             BackgroundColor="LightSkyBlue"
             xmlns:local="clr-namespace:TipoPaginaXamarin"
             >
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" >
            <Label Text="Tipo de página" TextColor="Blue"
                
                HorizontalOptions="CenterAndExpand" />
            <Button ImageSource="{local:ImageResource TipoPaginaXamarin.images.play.png}"
                    BackgroundColor="Transparent"
                    Clicked="MudarPagina"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>


 